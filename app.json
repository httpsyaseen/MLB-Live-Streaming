    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/baseball.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/base4.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.httpsyaseen.mlblivestreaming"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/baseball.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.httpsyaseen.mlblivestreaming",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/tv.png",
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "36075786-4f99-4bfd-bdad-b54066bb64e8"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-8966222560404211~8109902537"
  }
}
